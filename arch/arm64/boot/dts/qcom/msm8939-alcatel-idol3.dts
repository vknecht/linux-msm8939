/dts-v1/;

#include "msm8939.dtsi"
#include "pm8916.dtsi"
#include <dt-bindings/gpio/gpio.h>
#include <dt-bindings/input/input.h>
#include <dt-bindings/pinctrl/qcom,pmic-mpp.h>

#define DRM 1
#define FBSIMPLE 1
#define REGLCD 1
#define REGLCDSUPPLY 1
#define FAN53555 0
#define PWMBACKLIGHT 0
#define MDSSTE 0
#define I2C2GYRO 0
#define PM8916CODEC 1

#if DRM == 0
/ {
	/delete-node/ gpu_iommu;
	/delete-node/ gpu@1c00000;
};
#endif

/ {
	model = "Alcatel OneTouch Idol 3 (5.5)";
	compatible = "alcatel,idol3", "qcom,msm8939-mtp", "qcom,msm8939", "qcom,mtp";
	qcom,msm-id = <239 0>;
	qcom,board-id = <8 0>;

	aliases {
#if FBSIMPLE
		display0 = &framebuffer0; //&dsi0;
#endif
		serial0 = &blsp1_uart2;
	};

	chosen {
		//bootargs = "drm.debug=0x1ff loglevel=10 earlycon=tty0 console=tty0";
		bootargs = "drm.debug=12 loglevel=10 earlycon=tty0 console=tty0";
		//bootargs = "earlycon=tty0 console=tty0";
		stdout-path = "display0";

		#address-cells = <2>;
		#size-cells = <2>;
		ranges;
#if FBSIMPLE
		framebuffer0: framebuffer@83000000 {
			status= "okay";
			compatible = "simple-framebuffer";
			reg = <0x0 0x83200000 0x0 (1080 * 1920 * 3)>;
			width = <1080>;
			height = <1920>;
			stride = <(1080 * 3)>;
			format = "r8g8b8";
			power-domains = <&gcc MDSS_GDSC>;
			clocks = <&gcc GCC_MDSS_AHB_CLK>,
				 <&gcc GCC_MDSS_VSYNC_CLK>,
				 <&gcc GCC_MDSS_AXI_CLK>,
				 <&gcc GCC_MDSS_MDP_CLK>,
				 <&gcc GCC_MDSS_BYTE0_CLK>,
				 <&gcc GCC_MDSS_PCLK0_CLK>,
				 <&gcc GCC_MDSS_ESC0_CLK>,
				 <&xo_board>;
		};
#endif
	};

	reserved-memory {
		ramoops: ramoops_mem_region@9ff00000 {
			compatible = "ramoops";
			reg = <0 0x9ff00000 0 0x100000>;

			record-size = <0x1000>;
			console-size = <0x40000>;
			ftrace-size = <0x1000>;
			//pmsg-size = <0x20000>;
		};
#if FBSIMPLE
		cont_splash_mem: framebuffer@83000000 {
#else
		cont_splash_mem: splash_region@83000000 {
#endif
			compatible = "removed-dma-pool";
			reg = <0x0 0x83000000 0x0 0x2000000>; //(1080 * 1920 * 3)>;
			no-map;
		};
	};

	soc {
		sdhci@7824000 {
			status = "okay";

			vmmc-supply = <&pm8916_l8>;
			vqmmc-supply = <&pm8916_l5>;

			pinctrl-names = "default", "sleep";
			pinctrl-0 = <&sdc1_clk_on &sdc1_cmd_on &sdc1_data_on>;
			pinctrl-1 = <&sdc1_clk_off &sdc1_cmd_off &sdc1_data_off>;
		};

		sdhci@7864000 {
			status = "disabled";

			vmmc-supply = <&pm8916_l11>;
			vqmmc-supply = <&pm8916_l12>;

			pinctrl-names = "default", "sleep";
			pinctrl-0 = <&sdc2_clk_on &sdc2_cmd_on &sdc2_data_on>;
			pinctrl-1 = <&sdc2_clk_off &sdc2_cmd_off &sdc2_data_off>;
		};

		//serial@78b0000 {
		//	status = "okay";
		//	pinctrl-names = "default", "sleep";
		//	pinctrl-0 = <&blsp1_uart2_default>;
		//	pinctrl-1 = <&blsp1_uart2_sleep>;
		//};

		usb@78d9000 {
			status = "okay";
			extcon = <&usb_id>, <&usb_id>;

			hnp-disable;
			srp-disable;
			adp-disable;

			ulpi {
				phy {
					extcon = <&usb_id>;
					v1p8-supply = <&pm8916_l7>;
					v3p3-supply = <&pm8916_l13>;
				};
			};
		};

		usb_id: usb-id {
			compatible = "linux,extcon-usb-gpio";
			id-gpio = <&msmgpio 110 GPIO_ACTIVE_HIGH>;
			pinctrl-names = "default";
			pinctrl-0 = <&usb_id_default>;
		};

		mdss@1a00000 {
			status = "ok";
			dsi@1a98000 {
				#address-cells = <1>;
				#size-cells = <0>;

				vdda-supply = <&pm8916_l2>;
				vddio-supply = <&pm8916_l6>;
			};

			dsi-phy@1a98300 {
				vddio-supply = <&pm8916_l6>;
				qcom,dsi-phy-regulator-ldo-mode;
			};
		};

		i2c@78b9000 {
			synaptics@20 {
				compatible = "syna,rmi4-i2c";
				reg = <0x20>;
				pinctrl-names = "default";
				pinctrl-0 = <&ts_pins>;
				interrupt-parent = <&msmgpio>;
				interrupts = <13 0x2008>;
				reset-gpios = <&msmgpio 12 GPIO_ACTIVE_LOW>;
				#address-cells = <1>;
				#size-cells = <0>;

				rmi4-f01@01 {
					reg = <0x1>;
					syna,nosleep-mode = <1>;
				};

				rmi4-f11@11 {
					reg = <0x11>;
					syna,sensor-type = <1>;
				};
			};

			edt-ft5x06@38 {
				status = "okay";
				compatible = "edt,edt-ft5406";
				reg = <0x38>;
				pinctrl-names = "default";
				pinctrl-0 = <&ts_pins>;
				interrupt-parent = <&msmgpio>;
				interrupts = <13 0x2008>;
				reset-gpios = <&msmgpio 12 GPIO_ACTIVE_LOW>;

				touchscreen-size-x = <1080>;
				touchscreen-size-y = <1920>;

				wakeup-source;
			};
		};

		i2c@78b8000 {
			ncp6335d: ncp6335d-regulator@1c {
				compatible = "onnn,ncp6335d";
				reg = <0x1c>;
				pinctrl-names = "default";
				pinctrl-0 = <&ext_buck_vsel>;
				regulator-min-microvolt = <1050000>;
				regulator-max-microvolt = <1350000>;
				regulator-always-on; // otherwise device ~shuts off after ~30s
				onnn,vsel = <0>;
				onnn,slew-ns = <3333>;
				onnn,step-size = <6250>;
				onnn,min-slew-ns = <416>;
				onnn,max-slew-ns = <3333>;
				onnn,min-setpoint = <600000>;
				onnn,vsel-gpio = <&msmgpio 111 1>;
				onnn,discharge-enable;
				onnn,restore-reg;
			};
#if FAN53555
			reg_arm_dram: regulator@60 {
				compatible = "fcs,fan53555";
				pinctrl-names = "default";
				pinctrl-0 = <&ext_buck_vsel>;
				reg = <0x60>;
				regulator-min-microvolt =  <1050000>;
				regulator-max-microvolt = <1350000>;
				regulator-ramp-delay = <1250>;
				regulator-always-on;
				vsel-gpios = <&msmgpio 111 1>;
			};
#endif
		};

		wcnss@a21b000 {
			status = "okay";

			iris {
				compatible = "qcom,wcn3680";
			};
		};

		/*
		 * Attempting to enable these devices causes a "synchronous
		 * external abort". Suspected cause is that the debug power
		 * domain is not enabled by default on this device.
		 * Disable these devices for now to avoid the crash.
		 *
		 * See: https://lore.kernel.org/linux-arm-msm/20190618202623.GA53651@gerhold.net/
		 */
		tpiu@820000 { status = "disabled"; };
		funnel@821000 { status = "disabled"; };
		replicator@824000 { status = "disabled"; };
		etf@825000 { status = "disabled"; };
		etr@826000 { status = "disabled"; };
		funnel@841000 { status = "disabled"; };
		debug@850000 { status = "disabled"; };
		debug@852000 { status = "disabled"; };
		debug@854000 { status = "disabled"; };
		debug@856000 { status = "disabled"; };
		etm@85c000 { status = "disabled"; };
		etm@85d000 { status = "disabled"; };
		etm@85e000 { status = "disabled"; };
		etm@85f000 { status = "disabled"; };
	};

#if PWMBACKLIGHT
	backlight: backlight {
		compatible = "pwm-backlight";
		pwms = <&pm8916_pwm 0 255000>;
		brightness-levels = <0 255>;
		num-interpolated-steps = <255>;
		default-brightness-level = <255>;
	};
#endif

	gpio-keys {
		compatible = "gpio-keys";
		input-name = "gpio-keys";
		#address-cells = <1>;
		#size-cells = <0>;
		autorepeat;

		label = "GPIO Buttons";

		button@0 {
			label = "Volume Up";
			gpios = <&msmgpio 107 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			wakeup-source;
			debounce-interval = <15>;
		};
	};

#if REGLCD
	reg_lcd_enp: regulator-lcd-enp {
		compatible = "regulator-fixed";
		regulator-name = "lcd_enp";
		regulator-min-microvolt = <1800000>; // Arbitrary value
		regulator-max-microvolt = <1800000>;
		//regulator-always-on;
		gpio = <&msmgpio 105 0>;
		enable-active-high;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_enp_default>;
	};

	reg_lcd_enn: regulator-lcd-enn {
		compatible = "regulator-fixed";
		regulator-name = "lcd_enn";
		regulator-min-microvolt = <1800000>; // Arbitrary value
		regulator-max-microvolt = <1800000>;
		//regulator-always-on;
		gpio = <&msmgpio 106 0>;
		enable-active-high;
		pinctrl-names = "default";
		pinctrl-0 = <&lcd_enn_default>;
	};
#endif // REGLCD

};

#if I2C2GYRO
&blsp_i2c2 {
	status = "okay";
	//tsl2772@39 {
	//};
	gyroscope@68 {
		compatible = "bosch,bmg160";
		reg = <0x68>;
		interrupt-parent = <&msmgpio>;
		interrupts = <23 IRQ_TYPE_EDGE_RISING>;

		//vdd-supply = <&pm8916_l17>;
		//vddio-supply = <&pm8916_l6>;
	};
};
#endif // I2C2GYRO

&msmgpio {

	ts_pins: ts_pins {
		ts_int_active: pmx_ts_int_active {
			pinmux {
				pins = "gpio13";
				function = "gpio";
			};
			
			pinconf {
				pins = "gpio13";
				drive-strength = <16>;
				bias-pull-up;
			};
		};

		ts_int_suspend: pmx_ts_int_suspend {
			pinmux {
				pins = "gpio13";
				function = "gpio";
			};

			pinconf {
				pins = "gpio13";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		ts_reset_active: pmx_ts_reset_active {
			pinmux {
				pins = "gpio12";
				function = "gpio";
			};

			pinconf {
				pins = "gpio12";
				drive-strength = <16>;
				bias-pull-up;
			};
		};

		ts_reset_suspend: pmx_ts_reset_suspend {
			pinmux {
				pins = "gpio12";
				function = "gpio";
			};

			pinconf {
				pins = "gpio12";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		ts_release: pmx_ts_release {
			pinmux{
				pins = "gpio13", "gpio12";
				label = "pmx_ts_release";
			};
			
			pinconf {
				pins = "gpio13", "gpio12";
				drive-strength = <2>;
				bias-pull-down;
			};
		};
	};

	pmx_mdss {
		mdss_default: mdss_default {
			pinmux {
				function = "gpio";
				pins = "gpio25";
			};
			pinconf {
				pins = "gpio25";
				drive-strength = <8>;
				bias-disable;
			};
		};

		mdss_sleep: mdss_sleep {
			pinmux {
				function = "gpio";
				pins = "gpio25";
			};
			pinconf {
				pins = "gpio25";
				drive-strength = <2>;
				bias-pull-down;
			};
		};
	};

#if MDSSTE
	pmx_mdss_te {
		mdss_te_default: mdss_te_default {
			pinmux {
				function = "gpio";
				pins = "gpio24";
			};
			pinconf {
				pins = "gpio24";
				drive-strength = <2>;
				bias-pull-down;
			};
		};

		mdss_te_sleep: mdss_te_sleep {
			pinmux {
				function = "gpio";
				pins = "gpio24";
			};
			pinconf {
				pins = "gpio24";
				drive-strength = <2>;
				bias-pull-down;
			};
		};
	};
#endif // MDSSTE

#if REGLCD
	lcd_enp_default: lcd_enp_default {
		pinmux {
			function = "gpio";
			pins = "gpio105";
		};
		pinconf {
			pins = "gpio105";
			drive-strength = <2>;
			bias-pull-up;
			output-high;
		};
	};

	lcd_enn_default: lcd_enn_default {
		pinmux {
			function = "gpio";
			pins = "gpio106";
		};
		pinconf {
			pins = "gpio106";
			drive-strength = <2>;
			bias-pull-up;
			output-high;
		};
	};
#endif // REGLCD

	ext_buck_vsel: ext_buck_vsel {
		pinmux {
			function = "gpio";
			pins = "gpio111";
		};
		pinconf{
			pins = "gpio111";
			drive-strength = <2>;
		};
	};

	usb_id_default: usb_id_default {
                pinmux {
                        function = "gpio";
                        pins = "gpio110";
                };
                pinconf {
                        pins = "gpio110";
                        drive-strength = <8>;
                        bias-pull-up;
                };
        };

};
#if PWMBACKLIGHT
&pm8916_mpps {
	pwm_out: pm8916_mpp4 {
		pinconf {
			pins = "mpp4";
			function = "digital";
			output-low;
			power-source = <PM8916_MPP_L5>; // _VPH on other msm8916
			qcom,dtest = <1>;
		};
	};
};
#endif // PWMBACKLIGHT

&spmi_bus {
 	pm8916@0 {
 		pon@800 {
 			volume-down {
 				compatible = "qcom,pm8941-resin";
 				interrupts = <0x0 0x8 1 IRQ_TYPE_EDGE_BOTH>;
 				bias-pull-up;
 				linux,code = <KEY_VOLUMEDOWN>;
 			};
 		};
 	};

	pm8916@1 {

#if PWMBACKLIGHT
		pwm@bc00 {
			status = "okay";
			pinctrl-names = "default";
			pinctrl-0 = <&pwm_out>;
		};
#endif

		vibrator@c000 {
			status = "okay";
		};

#if PM8916CODEC
		codec@f000 {
			qcom,micbias-lvl = <2800>;
			qcom,mbhc-vthreshold-low = <75 150 237 450 500>;
			qcom,mbhc-vthreshold-high = <75 150 237 450 500>;
		};
#endif
	};
};

&smd_rpm_regulators {
	vdd_l1_l2_l3-supply = <&pm8916_s3>;
	vdd_l4_l5_l6-supply = <&pm8916_s4>;
	vdd_l7-supply = <&pm8916_s4>;

	s1 {
		regulator-min-microvolt = <500000>;
		regulator-max-microvolt = <1300000>;
	};

	s3 {
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1300000>;
	};

	s4 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <2100000>;
	};

	l1 {
		regulator-min-microvolt = <1225000>;
		regulator-max-microvolt = <1225000>;
	};

	l2 {
		regulator-min-microvolt = <1200000>;
		regulator-max-microvolt = <1200000>;
	};

	l3 {
		regulator-min-microvolt = <500000>;
		regulator-max-microvolt = <1287500>;
	};

	l4 {
		regulator-min-microvolt = <2050000>;
		regulator-max-microvolt = <2050000>;
	};

	l5 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	l6 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
		regulator-always-on;
	};

	l7 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <1800000>;
	};

	l8 {
		regulator-min-microvolt = <2850000>;
		regulator-max-microvolt = <2900000>;
	};

	l9 {
		regulator-min-microvolt = <3300000>;
		regulator-max-microvolt = <3300000>;
	};

	l10 {
		regulator-min-microvolt = <2700000>;
		regulator-max-microvolt = <2800000>;
	};

	l11 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <2950000>;
		regulator-allow-set-load;
		regulator-system-load = <200000>;
	};

	l12 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <2950000>;
	};

	l13 {
		regulator-min-microvolt = <3075000>;
		regulator-max-microvolt = <3075000>;
	};

	l14 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
	};

	l15 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
	};

	l16 {
		regulator-min-microvolt = <1800000>;
		regulator-max-microvolt = <3300000>;
	};

	l17 {
		regulator-min-microvolt = <2850000>;
		regulator-max-microvolt = <2850000>;
	};

	l18 {
		regulator-min-microvolt = <2700000>;
		regulator-max-microvolt = <2700000>;
	};
};

#if DRM
&dsi0 {
	pinctrl-names = "default", "sleep";
	pinctrl-0 = <&mdss_default>;
	pinctrl-1 = <&mdss_sleep>;

	panel@0 {
		//compatible = "mdss,r63315"; //"sharp,r63315";
		compatible = "mdss,nt35596";
		reg = <0>;

		reset-gpios = <&msmgpio 25 GPIO_ACTIVE_HIGH>;
		power-supply = <&pm8916_l17>;
		//vdd-supply = <&pm8916_l8>;
		//vddio-supply = <&pm8916_l6>;
#if REGLCDSUPPLY
		enp-supply = <&reg_lcd_enp>;
		enn-supply = <&reg_lcd_enn>;
#endif
#if PWMBACKLIGHT
		backlight = <&backlight>;
#endif
		ports {
			#address-cells = <1>;
			#size-cells = <0>;

			port@0 {
				reg = <0>;
				panel_in: endpoint {
					remote-endpoint = <&dsi0_out>;
				};
			};
		};
	};

	ports {
		port@1 {
			endpoint {
				remote-endpoint = <&panel_in>;
				data-lanes = <3 0 1 2>;
			};
		};
	};
};
#endif // DRM
